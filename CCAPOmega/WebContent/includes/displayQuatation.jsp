<?xml version="1.0" encoding="ISO-8859-1" ?>
<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<%@page import="com.omega.dtd.Vehicle"%>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
<title>Finally got some response</title>
</head>
<body>
<jsp:useBean id="insureVehicle" scope="request"
	class="com.omega.dtd.Vehicle" />
<div>Customer Information</div>
<table>
	<tr>
		<td>Name:</td>
		<td><input class="input" type="text" name="name"
			readonly="readonly" value="${insureVehicle.name}" /></td>
	</tr>
	<tr>
		<td>SSN:</td>
		<td><input class="input" type="text" name="SSN"
			readonly="readonly" value="${insureVehicle.ssn}" /></td>
	</tr>
	<tr>
		<td>AGE:</td>
		<td><input class="input" type="text" name="age"
			readonly="readonly" value="${insureVehicle.age}" /></td>
	</tr>
	<tr>
		<td>Gender :</td>
		<td><input class="input" type="text" name="gender"
			readonly="readonly" value="${insureVehicle.gender}" /></td>
	</tr>
	<tr>
		<td>Vehicle Model:</td>
		<td><input class="input" type="text" name="model"
			readonly="readonly" value="${insureVehicle.vehicleModel}" /></td>
	</tr>
	<tr>
		<td>Vehicle Make:</td>
		<td><input class="input" type="text" name="make"
			readonly="readonly" value="${insureVehicle.make}" /></td>
	</tr>
	<tr>
		<td>Year:</td>
		<td><input class="input" type="text" name="year"
			readonly="readonly" value="${insureVehicle.year}" /></td>
	</tr>
	<tr>
		<td>No Accidents last year:</td>
		<td><input class="input" type="text" name="lstnoofaccidents"
			readonly="readonly" value="${insureVehicle.lstYearAccidents}" /></td>
	</tr>
	<tr>
		<td>Coverage Type:</td>
		<td><input class="input" type="text" name="coverageType"
			readonly="readonly" value="${insureVehicle.coverageType}" /></td>
	</tr>
	<tr>
		<td>Coverage Amount:</td>
		<td><input class="input" type="text" name="coverageAmount"
			readonly="readonly" value="${insureVehicle.coverageAmount}" /></td>
	</tr>
	<tr>
		<td>Premium:</td>
		<td><input class="input" type="text" name="premium"
			readonly="readonly" value="${insureVehicle.premium}" /></td>
	</tr>
</table>
</body>
</html>