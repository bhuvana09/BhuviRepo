<?xml version="1.0" encoding="ISO-8859-1" ?>
<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:xforms="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
<title>Starting Data Collection</title>

</head>
<body>

<form action="collectionController" method="get"
	onsubmit="return validateOnSubmit(this)">
<table>
	<tbody>
		<tr>
			<td>SSN:</td>
			<td><input name="SSN" /></td>
		</tr>
		<tr>
			<td>Name:</td>
			<td><input name="name" /><br />
			</td>
		</tr>
		<tr>
			<td>Age</td>
			<td><input name="age" /></td>
		</tr>
		<tr>
			<td>Gender</td>
			<td><select name="gender">
				<option value="Male">Male</option>
				<option value="Female">Female</option>
			</select></td>
		</tr>

		<tr>
			<td>Vehicle Model:</td>
			<td><input name="model" /></td>
		</tr>
		<tr>
			<td>Make:</td>
			<td><input name="make" /></td>
		</tr>
		<tr>
			<td>Year</td>
			<td><select name="year">
				<option value="2010">2010</option>
				<option value="2009">2009</option>
				<option value="2008">2008</option>
				<option value="2007">2007</option>
				<option value="2006">2006</option>
				<option value="2005">2005</option>
				<option value="2004">2004</option>
				<option value="2003">2003</option>
				<option value="2002">2002</option>
				<option value="2001">2001</option>
				<option value="2000">2000</option>
				<option value="1999">1999</option>
				<option value="1998">1998</option>
				<option value="1997">1997</option>
				<option value="1996">1996</option>
				<option value="1995">1995</option>
				<option value="1994">1994</option>
				<option value="1993">1993</option>
				<option value="1992">1992</option>
				<option value="1991">1991</option>
				<option value="1990">1990</option>
				<option value="1989">1989</option>
				<option value="1988">1988</option>
				<option value="1987">1987</option>
				<option value="1986">1986</option>
				<option value="1985">1985</option>
				<option value="1984">1984</option>
				<option value="1983">1983</option>
				<option value="1982">1982</option>
				<option value="1981">1981</option>
				<option value="1980">1980</option>
				<option value="1979">1979</option>
				<option value="1978">1978</option>
				<option value="1977">1977</option>
				<option value="1976">1976</option>
				<option value="1975">1975</option>
				<option value="1974">1974</option>
				<option value="1973">1973</option>
				<option value="1972">1972</option>
				<option value="1971">1971</option>
				<option value="1970">1970</option>
			</select></td>
		</tr>
		<tr>
			<td>Driving license Number:</td>
			<td><input name="licenseNumber" /></td>
		</tr>
		<tr>
			<td>No: of accidents last user:</td>
			<td><select id=accident class="noOfAccidents"
				name="noOfAccidents"}>
				<option value="0">0</option>
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
			</select></td>
		</tr>
		<tr>
			<td>CoverageType:</td>
			<td><select name="coverageType" onchange="">
				<option value="BI">Bodily Injury</option>
				<option value="CL">Collision</option>
				<option value="MP">Medical Payment</option>
				<option value="PD">Physical Damage</option>

			</select></td>
		</tr>
		<tr>
			<td>CoverageAmount:</td>
			<td><select name="amount" onchange="">
				<option value="5000">5000</option>
				<option value="10000">10000</option>
				<option value="15000">15000</option>
			</select></td>
		</tr>
		<tr>
			<td>Click to Submit:</td>
			<td>
			<button type="submit" value="Submit Form" name="Submit Form">Submit
			Form</button>
			</td>
		</tr>

	</tbody>
</table>
</form>

</body>
</html>